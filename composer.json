{
  "name": "yashiroiori/laravel-package-maker",
  "description": "Get a package skeleton and all other `make` commands from laravel base for package development.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "laravel"
  ],
  "authors": [
    {
      "name": "LI Javier Santiago",
      "email": "yashiroiori@yahoo.com"
    }
  ],
  "require": {
    "illuminate/support": "~10.0|~11.0",
    "illuminate/console": "~10.0|~11.0",
    "illuminate/filesystem": "~10.0|~11.0"
  },
  "autoload": {
    "psr-4": {
      "Yashiroiori\\LaravelPackageMaker\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yashiroiori\\LaravelPackageMaker\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yashiroiori\\LaravelPackageMaker\\LaravelPackageMakerServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^3.8",
    "mockery/mockery": "^1.2"
  }
}
